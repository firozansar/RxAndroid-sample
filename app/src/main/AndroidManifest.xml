<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.firozansari.rxandroid_sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ColorListActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity
            android:name=".TVShowsActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity
            android:name=".ImprovedTVShowsActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity
            android:name=".ButtonCounterActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity
            android:name=".ValueDisplayActivity"
            android:parentActivityName=".MainActivity"
            />

        <activity
            android:name=".CitySearchActivity"
            android:parentActivityName=".MainActivity"
            />
    </application>

</manifest>